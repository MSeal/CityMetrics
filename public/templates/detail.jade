div
  .citytitle <%- cities[0] %>
  //
    table.suggestions
      tr
        td(style='color: #888; text-align: right') similar cities
        td.metricsimilar
          a(href='#') Houston
          a(href='#') Denver
          a(href='#') Sacramento
          a(href='#') Phoenix

  table.metrics
    | <% _.each(data, function(val) { %>
    tr
      td.metricname <%- val.label %>
      td.metricvalue <%- val.value %>
      td.metricsimilar
        | <% _.each(val.sim, function(sim) { %>
        a(href='#') <%- sim %>
        | <% }) %>
    | <% }) %>
    